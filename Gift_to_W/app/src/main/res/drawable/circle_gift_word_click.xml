<?xml version="1.0" encoding="utf-8"?>
<!-- android:shape指定形状类型，默认为rectangle -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid
        android:color="@color/dark_light"
        />

    <!-- size设置形状的大小 -->
    <size
        android:width="30dp"
        android:height="30dp" />
